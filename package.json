{
  "name": "lambda-user-management",
  "version": "1.0.0",
  "description": "AWS Lambda User Management with TypeScript and Sequelize",
  "main": "dist/index.js",
  "sideEffects": true,
  "scripts": {
    "build": "webpack --mode=development",
    "build:dev": "webpack --mode=development",
    "watch": "webpack --mode=development --watch",
    "sam:build": "sam build",
    "sam:local": "bash -c 'set -a; source .env; set +a; echo \"Environment variables:\"; echo \"DATABASE_HOST=$DATABASE_HOST\"; echo \"ENVIRONMENT=$ENVIRONMENT\"; sam local start-api --port 3000 --host 0.0.0.0 --parameter-overrides Environment=$ENVIRONMENT DatabaseHost=$DATABASE_HOST DatabasePort=$DATABASE_PORT DatabaseName=$DATABASE_NAME DatabaseUser=$DATABASE_USER DatabasePassword=$DATABASE_PASSWORD JWTSecret=$JWT_SECRET JWTExpiresIn=$JWT_EXPIRES_IN'",
    "sam:invoke": "sam local invoke",
    "sam:deploy:dev": "sam deploy --config-env dev",
    "sam:deploy:prod": "sam deploy --config-env prod",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:setup": "./scripts/setup-local-db.sh",
    "db:migrate": "ts-node scripts/run-migrations.ts",
    "db:seed": "ts-node scripts/seed-data.ts",
    "local:setup": "./scripts/setup-local.sh",
    "install:layers": "cd layers/common-layer/nodejs && npm install && cd ../../sequelize-layer/nodejs && npm install && cd ../../..",
    "test:register": "./scripts/test-register.sh",
    "test:login": "./scripts/test-login.sh",
    "docker:up": "docker-compose -f docker/docker-compose.yml up -d",
    "docker:down": "docker-compose -f docker/docker-compose.yml down",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "aws",
    "lambda",
    "typescript",
    "sequelize",
    "postgresql"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "effect": "^3.17.14",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.126",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.9.0",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
